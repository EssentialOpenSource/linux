		dsi_dual_jdi_a407_cmd_config0: config0 {
			qcom,split-mode = "dualctl-split";
		};

		dsi_dual_jdi_a407_cmd_config1: config1 {
			qcom,split-mode = "pingpong-split";
		};
	};
};
