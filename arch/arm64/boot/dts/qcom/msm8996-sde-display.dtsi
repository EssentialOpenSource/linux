		vddio-supply = <&pm8994_l14>;
		vdd-supply = <&pm8994_l19>;
	};

	dsi_jdi_1080p_vid: qcom,dsi-display@3 {
		compatible = "qcom,dsi-display";
		label = "single_dsi_jdi_1080p";
		qcom,display-type = "primary";

		qcom,dsi-ctrl = <&sde_dsi0>;
