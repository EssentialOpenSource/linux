			"AMIC4", "MIC BIAS2",
			"MIC BIAS2", "ANCRight Headset Mic",
			"AMIC5", "MIC BIAS3",
			"MIC BIAS3", "Analog Mic6",
			"AMIC6", "MIC BIAS4",
			"MIC BIAS4", "Analog Mic7",
			"DMIC0", "MIC BIAS1",
			"MIC BIAS1", "Digital Mic0",
			"DMIC1", "MIC BIAS3",
			"MIC BIAS3", "Digital Mic1",
			"DMIC2", "MIC BIAS4",
