				mux {
					pins = "gpio105";
					function = "gpio";
					bias-pull-down; /* pull down */
				};

				config {
					pins = "gpio105";
					drive-strength = <16>; /* 16 mA */
					bias-pull-down; /* pull down */
				};
			};

			adv7533_0_switch_suspend: adv7533_0_switch_suspend {
